{
  "name": "monopoly",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "NODE_ENV=development nodemon ./bin/www",
    "db:create:migration": "sequelize migration:generate --name ",
    "db:migrate": "sequelize db:migrate",
    "db:rollback": "sequelize db:migrate:undo",
    "build": "webpack || true",
    "build:dev": "webpack --progress --colors --watch",
    "postinstall": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "precommit": "npm run build"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "~1.18.2",
    "bootstrap-validate": "^1.0.11",
    "connect-flash": "^0.1.1",
    "connect-pg-simple": "^4.2.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^4.0.0",
    "expect": "^21.2.1",
    "express": "~4.15.5",
    "express-flash": "0.0.2",
    "express-session": "^1.15.6",
    "express-validator": "^4.3.0",
    "handlebars": "^4.0.11",
    "hbs": "~4.0.1",
    "mocha": "^4.0.1",
    "moment": "^2.19.2",
    "morgan": "~1.9.0",
    "multer": "^1.3.0",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "passport-local": "^1.0.0",
    "passport.socketio": "^3.7.0",
    "pg": "^7.4.0",
    "pg-promise": "^7.3.1",
    "sequelize": "^4.22.6",
    "sequelize-cli": "^3.0.0",
    "serve-favicon": "~2.4.5",
    "socket.io": "^2.0.4"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "nodemon": "^1.12.1",
    "webpack": "^3.8.1"
  }
}
